    "url": "https://github.com/mfcochauxlaberge/simple-contest-client/issues"
  },
  "homepage": "https://github.com/mfcochauxlaberge/simple-contest-client#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "css-loader": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "onchange": "^5.2.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1"
  }
}
